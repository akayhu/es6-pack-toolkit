<!DOCTYPE html>
<html>
  <head>
    <base href="/">
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=0">
    <title></title>
    <style>
      * {
        margin: 0;
        padding: 0;
        outline: 0;
      }

      body {
        padding: 50px 30px;
        font: 13px "Helvetica Neue", "Lucida Grande", "Arial";
        background: #ECE9E9 -webkit-gradient(linear, 0% 0%, 0% 100%, from(#fff), to(#ECE9E9));
        background: #ECE9E9 -moz-linear-gradient(top, #fff, #ECE9E9);
        background-repeat: no-repeat;
        color: #555;
        -webkit-font-smoothing: antialiased;
      }

      a {
        color: #555;
        text-decoration: none;
      }
      a:hover {
        color: #303030;
      }

      ul li {
        list-style: none;
      }

      #files li a:focus,
      #files li a:hover {
        background: rgba(255,255,255,0.65);
        border: 1px solid #ececec;
      }

      #files a {
        display: block;
        height: 25px;
        border: 1px solid transparent;
        -webkit-border-radius: 5px;
        -moz-border-radius: 5px;
        border-radius: 5px;
        overflow: hidden;
        white-space: nowrap;
      }

      #files .name {
        text-indent: 28px;
        line-height: 25px;
      }

      #files .icon {
        background-image: url(data:image/png;base64,);
        background-repeat: no-repeat;
        background-position: 8px 5px;
        display: inline-block;
      }
    </style>
  </head>
  <body>
    <ul id="files">
    <% for(var i=0; i<htmlWebpackPlugin.options.chunkList.length; i++) {%>  
      <li>
        <a href="<%= htmlWebpackPlugin.options.chunkList[i].link %>" title="<%= htmlWebpackPlugin.options.chunkList[i].link %>">
          <span class="icon name"><%= htmlWebpackPlugin.options.chunkList[i].text %></span>
        </a>
      </li>
    <% } %>
    </ul>
  </body>
</html>